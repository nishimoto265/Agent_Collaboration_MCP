  },
  "bugs": {
    "url": "https://github.com/nishimoto265/Agent_Collaboration_MCP/issues"
  },
  "homepage": "https://github.com/nishimoto265/Agent_Collaboration_MCP#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "zod": "^3.24.1"
  },
  "engines": {
