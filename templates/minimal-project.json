{
  "projectName": "minimal-project",
  "organization": {
    "type": "minimal",
    "structure": {
      "totalAgents": 6,
      "roles": [
        "coordinator",
        "agent-1",
        "agent-2", 
        "agent-3",
        "manager",
        "auth-helper"
      ]
    }
  },
  "defaults": {
    "excludedPaths": [
      "node_modules",
      ".git",
      "dist",
      "build",
      ".env",
      ".gitignore"
    ]
  },
  "agents": {
    "agent-0": {
      "role": "統括管理",
      "tasks": ["要件定義", "品質管理", "エージェント調整"]
    },
    "agent-1": {
      "role": "コア実装",
      "tasks": ["主要機能実装", "アーキテクチャ設計"]
    },
    "agent-2": {
      "role": "テスト実装", 
      "tasks": ["テストケース作成", "品質保証"]
    },
    "agent-3": {
      "role": "ドキュメント作成",
      "tasks": ["API文書", "使用方法説明"]
    },
    "agent-4": {
      "role": "プロジェクト管理",
      "tasks": ["全体調整", "最終判断"]
    },
    "agent-5": {
      "role": "認証サポート",
      "tasks": ["認証管理", "セキュリティ確保"]
    }
  }
}